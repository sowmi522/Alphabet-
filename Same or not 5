#include <stdio.h>
#include <stdbool.h>
int main()
{
    int N; 
    int element; 
    bool has_even = false;
    bool has_odd = false;
    scanf("%d", &N);    
    for (int i = 0; i < N; i++) 
    {
        scanf("%d", &element); 
        if (element % 2 == 0) 
        { 
            has_even = true;
        }
        else
        { 
            has_odd = true;
        }
        if (has_even && has_odd) 
        {
        }
    }    
    if (has_even && has_odd) 
    {
        printf("Mixed\n");
    } 
    else if (has_even) 
    {
        printf("Even\n");
    } 
    else if (has_odd)
    { 
        printf("Odd\n");
    }
    return 0;
}
