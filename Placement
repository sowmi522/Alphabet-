#include <stdio.h>
#include <string.h>

int main() {
    char name[100];
    int arrear;
    int cpga;

    
    scanf("%s", name);
    scanf("%d", &arrear);
    scanf("%d", &cpga);

    
    if ((arrear == 1 && cpga > 70) || ((arrear == 1 || arrear == 2) && cpga > 75)) {
        printf("Name of the Student:%s\n", name);
        printf("%s is Eligible for Placement\n", name);
    } else {
        printf("Name of the Student:%s\n", name);
        printf("%s is Not Eligible for Placement\n", name);
    }

    return 0;
}
