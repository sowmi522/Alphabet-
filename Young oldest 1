```c
#include <stdio.h>

int main() {
    int n, i, age;
    int youngest = 1000, oldest = -1;

    scanf("%d", &n);
    if (n <= 0) {
        printf("Invalid Input");
        return 0;
    }

    for (i = 0; i < n; i++) {
        scanf("%d", &age);
        if (age < 0) {
            printf("Invalid Input");
            return 0;
        }
        if (age < youngest) youngest = age;
        if (age > oldest) oldest = age;
    }

    printf("Youngest=%d\nOldest=%d", youngest, oldest);
    return 0;
}
```
