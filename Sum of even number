int main()
{
    int n, m;
    int sum_of_even_values = 0;
    scanf("%d", &n);
    scanf("%d", &m);
    if (n <= 0 || n >= 30 || m <= 0 || m >= 30 || n > m)
    {
        printf("Invalid Input\n");
        return 0; 
    }
    for (int i = n; i <= m; i++) 
    {
        if (i % 2 == 0)
        {
            sum_of_even_values += i; 
        }
    }
    printf("%d\n", sum_of_even_values);

    return 0;
}
